ALLOWED_EMAIL_DOMAIN=example.com
SLACK_WEBHOOK_URL=xxx
SLACK_TOKEN=xxx
ONESIGNAL_USER_AUTH_KEY=Your_One_Signal_Auth_Key
ONESIGNAL_API_KEY=Your_One_Signal_Api_Key
APP_ID=Your_One_Signal_App_Key
