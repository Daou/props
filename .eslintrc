{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "globals": {
    "registerComponent": true,
    "createComponent": true,
    "getComponent": true,
    "renderComponent": true
  },
  "rules": {
    "import/no-unresolved": [0],
    "import/no-extraneous-dependencies": [0]
  }
}
